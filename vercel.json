{
  "version": 2,
  "framework": null,
  "rewrites": [
    {
      "source": "/api/bot",
      "destination": "/app/telegram.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "app/telegram.js": {
      "runtime": "vercel-node@latest"
    }
  }
}
